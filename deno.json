{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@imcotton/totk-loci-sketch",
  "version": "0.2.3",
  "license": "MIT",
  "imports": {
    "@std/encoding": "jsr:@std/encoding@^1.0.3",
    "strip-indent": "npm:strip-indent@^4.0.0",
    "valibot": "npm:valibot@~0.41.0"
  },
  "exports": {
    "./main": "./main.ts",
    "./make": "./make.ts",
    ".": "./mod.ts"
  },
  "tasks": {
    "verify": "deno publish --dry-run --allow-dirty",
    "post": "./draft.ts"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true
  },
  "lock": false,
  "publish": {
    "include": [
      "./common.ts",
      "./mod.ts",
      "./main.ts",
      "./make.ts",
      "./LICENSE.txt",
      "./README.md",
      "./deno.json"
    ]
  }
}
