{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@imcotton/totk-loci-sketch",
  "version": "0.12.0-rc.1",
  "license": "MIT",
  "imports": {
    "@std/encoding": "jsr:@std/encoding@^1.0.3",
    "@libs/qrcode": "jsr:@libs/qrcode@~1.2.8",
    "@oslojs/otp": "npm:@oslojs/otp@~1.0.0",
    "hono": "jsr:@hono/hono@^4.6.3",
    "hono/valibot-validator": "./vendor/hono__valibot-validator__0.3.0.ts",
    "strip-indent": "npm:strip-indent@4.0.0",
    "valibot": "jsr:@valibot/valibot@~0.42.0"
  },
  "exports": {
    "./otpsec": "./otpsec.ts",
    "./app": "./app.tsx",
    "./main": "./main.ts",
    "./make": "./make.ts",
    ".": "./mod.ts"
  },
  "tasks": {
    "verify": "deno publish --dry-run --allow-dirty",
    "dev": "deno serve --env -E=MATAROA_API_KEY,OTP_SECRET -N=mataroa.blog:443,esm.sh:443 --watch mod.ts",
    "post": "./draft.ts"
  },
  "unstable": [
    "kv"
  ],
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx",
    "jsxImportSourceTypes": "hono/jsx",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true
  },
  "lock": false,
  "publish": {
    "include": [
      "./vendor/*",
      "./components/*",
      "./deploy/deno.ts",
      "./common.ts",
      "./utils.ts",
      "./otp.ts",
      "./otpsec.ts",
      "./either.ts",
      "./articles.ts",
      "./assets.ts",
      "./app.tsx",
      "./mod.ts",
      "./main.ts",
      "./make.ts",
      "./LICENSE.txt",
      "./README.md",
      "./deno.json"
    ]
  }
}
