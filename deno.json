{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@imcotton/totk-loci-sketch",
  "version": "0.5.1",
  "license": "MIT",
  "imports": {
    "@std/encoding": "jsr:@std/encoding@^1.0.3",
    "@libs/crypto": "jsr:@libs/crypto@~1.1.6",
    "@libs/qrcode": "jsr:@libs/qrcode@~1.2.8",
    "hono": "npm:hono@^4.6.1",
    "hono/valibot-validator": "npm:@hono/valibot-validator@~0.3.0",
    "strip-indent": "npm:strip-indent@^4.0.0",
    "valibot": "npm:valibot@~0.41.0"
  },
  "exports": {
    "./app": "./app.tsx",
    "./main": "./main.ts",
    "./make": "./make.ts",
    ".": "./mod.ts"
  },
  "tasks": {
    "verify": "deno publish --dry-run --allow-dirty",
    "dev": "deno serve --env -E=MATAROA_API_KEY,OTP_SECRET -N=mataroa.blog:443,esm.sh:443 --watch deploy/deno.ts",
    "post": "./draft.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx",
    "jsxImportSourceTypes": "hono/jsx",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true
  },
  "lock": false,
  "publish": {
    "include": [
      "./common.ts",
      "./draft-form.tsx",
      "./otp-setup.tsx",
      "./app.tsx",
      "./mod.ts",
      "./main.ts",
      "./make.ts",
      "./LICENSE.txt",
      "./README.md",
      "./deno.json"
    ]
  }
}
